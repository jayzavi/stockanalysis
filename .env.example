# OpenRouter API key from https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Tavily API key from https://app.tavily.com
TAVILY_API_KEY=

# Frontend testing: use mock agents (no API calls). Set to false for live LLMs.
USE_MOCK_AGENTS=true

# Optional: override default models (see https://openrouter.ai/docs/models)
# OPENROUTER_MODEL_ALPHA=anthropic/claude-opus-4.6
# OPENROUTER_MODEL_BETA=openai/gpt-5.2
# OPENROUTER_MODEL_GAMMA=google/gemini-3-pro
# OPENROUTER_MODEL_CHAIRMAN=anthropic/claude-sonnet-4

# --- Google Sign-In + Drive (memo history). Set in Vercel only; never commit secrets. ---
# Create OAuth 2.0 credentials at https://console.cloud.google.com/apis/credentials
# Enable Google Drive API. Use authorized redirect: https://YOUR_VERCEL_URL/api/auth/callback/google
NEXTAUTH_SECRET=
NEXTAUTH_URL=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# Your Drive folder ID (from the folder URL: .../folders/FOLDER_ID)
GOOGLE_DRIVE_MEMOS_FOLDER_ID=
